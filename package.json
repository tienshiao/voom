{
  "name": "voom",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.ts",
    "build:voom": "bun build --compile --minify --sourcemap ./src/index.ts --outfile voom",
    "build:voom:all": "bun run build-all.ts"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "react": "^19",
    "react-dom": "^19",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
